select * from dimension where name like '%Jinsi%'
delete from population 


select * from item where dhis2id='mtUMlCLFTTz'

select * from dimension where gid=21941


select * from boundary where dtype like 'REGION'

select count(distinct(facility_id)) from population

select b2.name,b1.name,f.name, f.dhis2id from facility f
	inner join boundary b on f.ward_gid=b.gid
	inner join boundary b1 on b.district_gid=b1.gid
	inner join boundary b2 on b1.region_gid=b2.gid
	where  f.gid=23669


select p.*, f.name from population p inner join facility f on p.facility_id=f.gid
 where f.dhis2id='Akdo5gIzo3s'
order by month,  ageValue,genderValue



select  sum(population) from boundary where dtype like 'REGION' limit 10
